{
  "log": {
    "level": "warn",
    "output": "box.log"
  },
  "dns": {
    "servers": [
      {
        "tag": "ali-dns",
        "address": "https://223.5.5.5/dns-query",
        "detour": "direct-out"
      }
    ],
    "final": "ali-dns",
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    }
  },
  "route": {
    "rule_set": [
      {
        "tag": "block",
        "type": "remote",
        "format": "binary",
        "url": "https://srs.chaimi.cc/block.srs",
        "download_detour": "direct"
      },
      {
        "tag": "direct",
        "type": "remote",
        "format": "binary",
        "url": "https://srs.chaimi.cc/direct.srs",
        "download_detour": "direct"
      },
      {
        "tag": "code",
        "type": "remote",
        "format": "source",
        "url": "https://gist.githubusercontent.com/yanshuangchen/524f9382aa15bc6b456ff9beb5a06982/raw/b582bc5895a75dd5774147ae4b2d9bb55a9104f2/code.json",
        "download_detour": "direct"
      },
      {
        "tag": "proxy",
        "type": "remote",
        "format": "binary",
        "url": "https://srs.chaimi.cc/proxy.srs",
        "download_detour": "direct"
      },
      {
        "tag": "steam",
        "type": "remote",
        "format": "binary",
        "url": "https://srs.chaimi.cc/steam.srs",
        "download_detour": "direct"
      },
      {
        "tag": "telegram",
        "type": "remote",
        "format": "binary",
        "url": "https://srs.chaimi.cc/telegram.srs",
        "download_detour": "direct"
      }
    ],
    "rules": [
      {
        "rule_set": "block",
        "action": "reject"
      },
      {
        "rule_set": [
          "direct",
          "steam",
          "code"
        ],
        "outbound": "direct"
      },
      {
        "rule_set": [
          "telegram",
          "proxy"
        ],
        "outbound": "proxy"
      }
    ],
    "final": "final"
  },
  "inbounds": [
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "127.0.0.1",
      "listen_port": 1080
    }
  ],
  "outbounds": [
    {
      "tag": "final",
      "type": "selector",
      "outbounds": [
        "direct-out",
        "proxy"
      ]
    },
    {
      "tag": "proxy",
      "type": "selector",
      "outbounds": [
        "auto",
        "direct-out",
        "{all}"
      ]
    },
    {
      "tag": "auto",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "10m",
      "tolerance": 50
    },
    {
      "tag": "direct",
      "type": "selector",
      "outbounds": [
        "direct-out",
        "proxy"
      ]
    },
    {
      "tag": "direct-out",
      "type": "direct"
    },
    {
      "tag": "dns-out",
      "type": "dns"
    },
    {
      "tag": "HongKong",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇭🇰|HK|hk|香港|港|HongKong"
          ]
        }
      ]
    },
    {
      "tag": "TaiWan",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇹🇼|TW|tw|台湾|臺灣|台|Taiwan"
          ]
        }
      ]
    },
    {
      "tag": "Singapore",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇸🇬|SG|sg|新加坡|狮|Singapore"
          ]
        }
      ]
    },
    {
      "tag": "Japan",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇯🇵|JP|jp|日本|日|Japan"
          ]
        }
      ]
    },
    {
      "tag": "America",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇺🇸|US|us|美国|美|United States"
          ]
        }
      ]
    },
    {
      "tag": "Others",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
            "🇭🇰|HK|hk|香港|香|🇹🇼|TW|tw|台湾|台|🇸🇬|SG|sg|新加坡|狮|🇯🇵|JP|jp|日本|日|🇺🇸|US|us|美国|美"
          ]
        }
      ]
    }
  ],
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": true
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "yacd",
      "external_ui_download_detour": "proxy"
    }
  }
}
