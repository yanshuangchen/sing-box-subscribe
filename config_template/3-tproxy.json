{
    "log": {
        "level": "warn",
        "output": "stdout"
    },
    "dns": {
        "servers": [
            {
                "tag": "ali-dns",
                "address": "https://223.5.5.5/dns-query",
                "detour": "direct-out"
            },
            {
                "tag": "fakeip",
                "address": "fakeip"
            }
        ],
        "rules": [
            {
                "outbound": "any",
                "server": "ali-dns"
            },
            {
                "rule_set": "block",
                "action": "reject"
            },
            {
                "query_type": "HTTPS",
                "rule_set": "proxy",
                "server": "block-dns",
                "action": "reject"
            },
            {
                "clash_mode": "direct",
                "server": "ali-dns"
            },
            {
                "rule_set": [
                    "direct",
                    "steam"
                ],
                "server": "ali-dns"
            },
            {
                "query_type": [
                    "A",
                    "AAAA"
                ],
                "rule_set": "proxy",
                "server": "fakeip"
            },
            {
                "action": "route-options",
                "query_type": "AAAA",
                "client_subnet": "223.167.89.93/24",
                "server": "ali-dns"
            }
        ],
        "final": "ali-dns",
        "fakeip": {
            "enabled": true,
            "inet4_range": "198.18.0.0/15",
            "inet6_range": "fc00::/18"
        }
    },
    "route": {
        "rule_set": [
            {
                "tag": "ai",
                "type": "remote",
                "format": "binary",
                "url": "https://srs.chaimi.cc/ai.srs",
                "download_detour": "direct"
            },
            {
                "tag": "block",
                "type": "remote",
                "format": "binary",
                "url": "https://srs.chaimi.cc/block.srs",
                "download_detour": "direct"
            },
            {
                "tag": "direct",
                "type": "remote",
                "format": "binary",
                "url": "https://srs.chaimi.cc/direct.srs",
                "download_detour": "direct"
            },
            {
                "tag": "proxy",
                "type": "remote",
                "format": "binary",
                "url": "https://srs.chaimi.cc/proxy.srs",
                "download_detour": "direct"
            },
            {
                "tag": "steam",
                "type": "remote",
                "format": "binary",
                "url": "https://srs.chaimi.cc/steam.srs",
                "download_detour": "direct"
            },
            {
                "tag": "telegram",
                "type": "remote",
                "format": "binary",
                "url": "https://srs.chaimi.cc/telegram.srs",
                "download_detour": "direct"
            }
        ],
        "rules": [
            {
                "port": 53,
                "action": "hijack-dns"
            },
            {
                "rule_set": "block",
                "action": "reject"
            },
            {
                "clash_mode": "direct",
                "outbound": "direct-out"
            },
            {
                "rule_set": [
                    "direct",
                    "steam"
                ],
                "outbound": "direct"
            },
            {
                "rule_set": [
                    "telegram",
                    "proxy"
                ],
                "outbound": "proxy"
            }
        ],
        "final": "final"
    },
    "inbounds": [
        {
            "tag": "tun-in",
            "type": "tun",
            "interface_name": "sing-box",
            "address": [
                "172.19.0.1/30",
                "fdfe:dcba:9876::1/126"
            ],
            "auto_route": true,
            "stack": "system",
            "route_address": [
                "198.18.0.0/15",
                "fc00::/18",
                "91.108.56.0/22",
                "91.108.4.0/22",
                "91.108.8.0/22",
                "91.108.16.0/22",
                "91.108.12.0/22",
                "149.154.160.0/20",
                "91.105.192.0/23",
                "91.108.20.0/22",
                "185.76.151.0/24",
                "2001:b28:f23d::/48",
                "2001:b28:f23f::/48",
                "2001:67c:4e8::/48",
                "2001:b28:f23c::/48",
                "2a0a:f280::/32",
                "8.220.66.0/24",
                "47.245.154.0/24",
                "47.245.184.0/24",
                "47.245.185.0/24"
            ]
        }
    ],
    "outbounds": [
        {
            "tag": "final",
            "type": "selector",
            "outbounds": [
                "direct-out",
                "proxy"
            ]
        },
        {
            "tag": "proxy",
            "type": "selector",
            "outbounds": [
                "auto",
                "direct-out",
                "{all}"
            ]
        },
        {
            "tag": "auto",
            "type": "urltest",
            "outbounds": [
                "{all}"
            ],
            "url": "http://www.gstatic.com/generate_204",
            "interval": "10m",
            "tolerance": 50
        },
        {
            "tag": "direct",
            "type": "selector",
            "outbounds": [
                "direct-out",
                "proxy"
            ]
        },
        {
            "tag": "block",
            "type": "selector",
            "outbounds": [
                "block-out",
                "proxy"
            ]
        },
        {
            "tag": "direct-out",
            "type": "direct"
        },
        {
            "tag": "block-out",
            "type": "block"
        },
        {
            "tag": "dns-out",
            "type": "dns"
        },
        {
            "tag": "HongKong",
            "type": "selector",
            "outbounds": [
                "{all}"
            ],
            "filter": [
                {
                    "action": "include",
                    "keywords": [
                        "🇭🇰|HK|hk|香港|港|HongKong"
                    ]
                }
            ]
        },
        {
            "tag": "TaiWan",
            "type": "selector",
            "outbounds": [
                "{all}"
            ],
            "filter": [
                {
                    "action": "include",
                    "keywords": [
                        "🇹🇼|TW|tw|台湾|臺灣|台|Taiwan"
                    ]
                }
            ]
        },
        {
            "tag": "Singapore",
            "type": "selector",
            "outbounds": [
                "{all}"
            ],
            "filter": [
                {
                    "action": "include",
                    "keywords": [
                        "🇸🇬|SG|sg|新加坡|狮|Singapore"
                    ]
                }
            ]
        },
        {
            "tag": "Japan",
            "type": "selector",
            "outbounds": [
                "{all}"
            ],
            "filter": [
                {
                    "action": "include",
                    "keywords": [
                        "🇯🇵|JP|jp|日本|日|Japan"
                    ]
                }
            ]
        },
        {
            "tag": "America",
            "type": "selector",
            "outbounds": [
                "{all}"
            ],
            "filter": [
                {
                    "action": "include",
                    "keywords": [
                        "🇺🇸|US|us|美国|美|United States"
                    ]
                }
            ]
        },
        {
            "tag": "Others",
            "type": "selector",
            "outbounds": [
                "{all}"
            ],
            "filter": [
                {
                    "action": "exclude",
                    "keywords": [
                        "🇭🇰|HK|hk|香港|香|🇹🇼|TW|tw|台湾|台|🇸🇬|SG|sg|新加坡|狮|🇯🇵|JP|jp|日本|日|🇺🇸|US|us|美国|美"
                    ]
                }
            ]
        }
    ],
    "experimental": {
        "cache_file": {
            "enabled": true,
            "store_fakeip": true,
            "store_rdrc": true
        },
        "clash_api": {
            "external_controller": "0.0.0.0:9090",
